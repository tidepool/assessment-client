<div class="inner">

    <a class="logo" href="#"></a>

    <span id="StepsRemainingHolder"></span>

    {{#showNav}}
        <nav>
            <ul>
                <li><a href="#About">About</a></li>
                <li><a href="#Team">Team</a></li>
                <li><a href="#Investors">Investors</a></li>
            </ul>
        </nav>
    {{/showNav}}


    {{#if loggedIn}}
        <div class="loginArea">
            {{#if isRegisteredUser }}
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <!--<i class="icon-user avatar"></i>-->
                        <span id="userName">{{userName}}</span>
                        <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu">
                        <li><a id="ShowModalProfile">
                            <i class="icon-user"></i>
                            Profile
                        </a></li>
                        <li><a id="logout">
                            <i class="icon-user off"></i>
                            Logout
                        </a></li>
                    </ul>
                </li>
            {{else}}
                <!--<span id="userName">{{userName}}</span>-->
            {{/if}}
        </div>
    {{else}}

    {{/if}}
    <div class="loginArea">
        <button id="ShowModalLogin" class="btn btn-primary btn-large" href="">
            <i class="icon-lock icon-large"></i>
            Log In
        </button>
    </div>
</div>




